alias ll='ls -lh'
alias la='ls -lha'
alias l='less -SI $1'
alias pa='ps aux | grep $1'
alias ..='cd ..'
alias ...='cd ../..'
alias h='head'
alias t='tail'
alias fts='firefox& thunderbird & skype &'
alias rm='rm -i'
alias rmr='rm -rfd'
alias git='/usr/local/git/bin/git'
alias slack='/usr/local/slack/Slack.app/Contents/MacOS/Slack'
alias lg='ll | grep $1'